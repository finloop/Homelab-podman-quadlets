# traefik.container
[Container]
Image=traefik:latest
Network=slirp4netns:port_handler=slirp4netns
PublishPort=8080:80
Volume=./etc:/etc/traefik/:ro
Volume=traefik-logs.volume:/logs

[Install]
WantedBy=default.target

[Service]
# Extend timeout to 15 minutes so pull doesn't fail
TimeoutStartSec=900
Restart=always

